<quadraturin name="default">
	<ekran>
		<xres>640</xres>
		<yres>480</yres>
		<anitaliasing>false</anitaliasing>
	</ekran>
	<input>
		<scroll-step>40</scroll-step>
		<scale-step>0.9</scale-step>
		<!-- 
		public static final int PRESSED = 1;
		public static final int TAPPED = 2;
		public static final int RELEASED = 3;
		public static final int DRAGGED = 4;	
	
		public static final int MOUSE_LEFT_BUTTON = -1;
		public static final int MOUSE_RIGHT_BUTTON = -2;
		
		* keyboard key values are identical to KeyEvent.getKeyCode() values.
	 -->
		<binding actionId="scroll-left"  buttonId="37" modeId="2" />
		<binding actionId="scroll-right" buttonId="39" modeId="2" />
		<binding actionId="scroll-up"    buttonId="38" modeId="2" />
		<binding actionId="scroll-down"  buttonId="40" modeId="2" />
		<binding actionId="zoom-in"      buttonId="33" modeId="2" />
		<binding actionId="zoom-out"     buttonId="34" modeId="2" />
	</input>
	<stage>
			<frame-length>1</frame-length>
			<max-fps>60</max-fps>
	</stage>
</quadraturin>